{
    "python.pythonPath": ".venv/bin/python3",
    "launch": {
        "configurations": [
            {
                "name": "Robot framework",
                "type": "python",
                "request": "launch",
                "program": "${workspaceFolder}/.venv/bin/robot",
                "args": [
                    "-d",
                    "output",
                    //"-i",
                    //"Teste",
                    "-v",
                    "headless:true",
                    "-v",
                    "navegador:chromium",
                    "Tests/Caminho feliz.robot",
                ],
                "console": "integratedTerminal"
            },
        ],
        "compounds": []
    },
    "editor.fontSize": 14,
    "cSpell.words": [
        "esquerda",
        "retirar"
    ]

}
